- name: getUsers
  description: Retrieve a list of users
  operationId: getUsers
  parameters:
    - name: Authorization
      in: header
      required: true
      schema:
        type: string
  response:
    '200':
      description: Successful response containing a list of users
      content:
        application/json:
          schema:
            type: array
            items:
              type: object
              properties:
                id:
                  type: integer
                username:
                  type: string
                email:
                  type: string
